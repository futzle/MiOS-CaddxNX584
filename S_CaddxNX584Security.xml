<scpd xmlns="urn:schemas-upnp-org:service-1-0">
  <specVersion>
    <major>1</major>
    <minor>0</minor>
  </specVersion>
  <serviceStateTable>
    <stateVariable allowRepeats="yes">
      <name>LastEventLog</name>
      <dataType>string</dataType>
    </stateVariable>
  </serviceStateTable>
  <actionList>
    <action>
      <name>ZoneScan</name>
      <argumentList>
        <argument>
          <name>Zone</name>
          <direction>in</direction>
          <relatedStateVariable>ui4</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>ZoneNameScan</name>
      <argumentList>
        <argument>
          <name>Zone</name>
          <direction>in</direction>
          <relatedStateVariable>ui4</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>UserScan</name>
      <argumentList>
        <argument>
          <name>MasterPIN</name>
          <direction>in</direction>
          <relatedStateVariable>string</relatedStateVariable>
        </argument>
        <argument>
          <name>User</name>
          <direction>in</direction>
          <relatedStateVariable>ui4</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>UserSetPIN</name>
      <argumentList>
        <argument>
          <name>MasterPIN</name>
          <direction>in</direction>
          <relatedStateVariable>string</relatedStateVariable>
        </argument>
        <argument>
          <name>User</name>
          <direction>in</direction>
          <relatedStateVariable>ui4</relatedStateVariable>
        </argument>
        <argument>
          <name>UserPIN</name>
          <direction>in</direction>
          <relatedStateVariable>string</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>LogEventScan</name>
      <argumentList>
        <argument>
          <name>StackPointer</name>
          <direction>in</direction>
          <relatedStateVariable>ui4</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
  </actionList>
</scpd>
